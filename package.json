{
  "name": "edutrack",
  "version": "1.0.0",
  "description": "Software para la gestión automatizada de asistencias estudiantiles mediante códigos QR",
  "scripts": {
    "dev": "next dev --turbopack",
    "check": "npm run format:check && npm run type-check",
    "build": "prisma generate && next build",
    "seed": "prisma db seed",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run type-check",
    "prepare": "husky",
    "postbuild": "next-sitemap",
    "postinstall": "prisma generate",
    "update": "ncu -u"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
